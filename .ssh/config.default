Host whatever
    User florian
    ForwardX11 yes
    LocalForward 3307 localhost:3306
    RemoteForward 19000 localhost:19000

Host *.amazonaws.com
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no

Host *
    Compression yes
    CompressionLevel 7
    Cipher blowfish
    ServerAliveInterval 600
    ControlMaster auto
    ControlPath /tmp/ssh-%r@%h:%p
