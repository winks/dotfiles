#!/bin/bash
killall trayer >/dev/null 2>&1
trayer --edge top --align right --SetDockType true --SetPartialStrut true \
      --expand true --widthtype percent --width 8 --transparent true --tint 0x161616 
      --height 20 --monitor 2 >/dev/null 2>&1 &
#      --height 20 --monitor primary >/dev/null 2>&1 &

